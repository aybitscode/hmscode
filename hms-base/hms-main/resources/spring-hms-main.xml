<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.0.xsd">
    <import resource="classpath:spring-hms-rest.xml"/>
    <import resource="classpath:spring-hms-domain.xml"/>
    <context:annotation-config/>

    <!-- Init-->
    <bean id="hmsRestHttpService" class="com.aybits.hms.sparkInit.HmsRestHttpService"/>
    <bean id="serviceStartup" class="com.aybits.hms.sparkInit.HmsRestServiceStartup"/>
    <bean id="hmsServiceMain" class="com.aybits.hms.sparkInit.HmsRestServiceMain"/>

    <!-- Config -->
    <bean id="hmsConfig" class="com.aybits.hms.arch.util.HmsConfig"/>
    <bean id="initDBCP" class="com.aybits.hms.arch.dbman.DBCPConnection"/>

</beans>